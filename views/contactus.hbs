<section style="background-color: #F2F4F9;">
            <div class="container">
                <div class="row bia">
                    <div class="sec-item">
                        <h2 class="sec-tit">مقر الوزارة</h2>
                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3385.2262868121866!2d35.905486814668215!3d31.954757232761594!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x151ca07c65529b9f%3A0x39274bf436bd42d!2sMinistry%20of%20Planning%20%26%20International%20Cooperation!5e0!3m2!1sen!2sjo!4v1602056554676!5m2!1sen!2sjo" width="600" height="450" frameborder="0" style="border:0; align-self: center;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
                        <h2 class="sec-tit">معلومات الاتصال
                        </h2>
                        <div class="contacts">
                            <div class="contact-item">
                                <h3>
                                    العنوان
                                </h3>
                                <a href="https://goo.gl/maps/uqZbBHDsuuXdB2DdA" target="_blanck">
                                <p>
                                    عمان,الدوار الثالث ،شارع توفيق أبو الهدى، مبنى رقم (4)
                                </p>
                                </a>
                            </div>
                            <div class="contact-item">
                                <h3>
                                    هاتف
                                </h3>
                                <a href="tel:(+962)654644466">
                                <p>
                                    +962 6546 4466
                                </p>
                                </a>
                            </div>
                            <div class="contact-item">
                                <h3>
                                    فاكس
                                </h3>
                                <a href="tel:(+962)65464934"> 
                                <p>
                                    +962 6546 4934
                                </p>
                                </a>
                            </div>
                        </div>
                        {{>formModalCon}}
                        <div class="contact-form">
                            <h2 class="sec-tit">راسلنا</h2>
                            <form action="/contactus" id="idForm" method="POST">
                                <div class="form-row">
                                    <div class="form-item">
                                        <label for="fname">الاسم الاول*</label>
                                        <input type="text" name="fname" minlength="2" required id="fname">
                                    </div>
                                    <div class="form-item">
                                        <label for="lname">اسم العائلة*</label>
                                        <input type="text" id="sname" minlength="2" required name="lname">
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-item">
                                        <label for="email">البريد الإلكتروني*</label>
                                        <input type="email" id="email" required name="email">
                                    </div>
                                    <div class="form-item">
                                        <label for="phone">رقم الهاتف</label>
                                        <input type="text" id="phone" minlength="10" required name="phone">
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-item" style="width: 100%;">
                                        <label for="address">العنوان</label>
                                        <input type="text" id="address" required name="address">
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-item" style="width: 38%;">
                                        <label for="city">المحافظة</label>
                                        <select name="city" required>
                                            <option value="" disabled="" selected="">اسم المحافظة</option>
                                            {{#if listExists}}
                                                {{#each cities}}
                                            <option value="{{city_id}}">{{city}}</option>
                                                {{/each}}
                                            {{/if}}
                                        </select> </div>
                                    <div class="form-item" style="width: 38%;">
                                        <label for="village">الحي / القرية</label>
                                        <input type="text" id="village" required name="village">
                                    </div>
                                    <div class="form-item" style="width: 18%;">
                                        <label for="postal">الرمز البريدي*</label>
                                        <input type="text" id="postal" required name="postal">
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-item" style="width: 100%;">
                                        <label for="topic">الموضوع</label>
                                        <input type="text" id="topic" required name="topic">
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-item" style="width: 100%;">
                                        <label for="message">الرسالة</label>
                                        <textarea name="message" id="message" required rows="10"></textarea>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-item" style="width: 100%;">
                                        <input type="submit" value="إرسال">
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>

                </div>
        {{>fillerRow}}
        </div>
        </section>