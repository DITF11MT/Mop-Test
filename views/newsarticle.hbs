<div class="container">
            <div class="page-title">
                <h3>
                    {{title}}
                </h3>
                <div class="navilinks">
                    <h4>
                        <a href="/">                            
                            {{parent}}
                        </a>
                    </h4>
                    <h4>
                        <a href="news" class="currentpage na">
                            {{page}}                            
                        </a>
                    </h4>                    
                </div>
            </div>
        </div>

        <section style="background-color: #F2F4F9;">
            <div class="container">
                <div class="row bia" style="text-align: right;">
                    <div class="sec-item" style="text-align: justify;">
                
                {{#each article}}
                <p>
                    {{news_date}}
                </p>
                <p>
                    {{news_info}}  
                </p>
                <p>
                    {{news_txt}}
                </p>
                <img src="news{{news_id}}.jpg" class="img-fluid big-img" alt="news">
                {{/each}}
                </div>
                </div>
                {{>fillerRow}}
                </div>
            </div>
        </section>